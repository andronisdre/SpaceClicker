{
  "name": "electronspaceclicker",
  "version": "1.0.0",
  "description": "spaceElectron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.example.SpaceClickerElectron",
    "productName": "SpaceClickerElectron",
    "directories": {
      "output": "dist"
    },
    "files": [
      "public/**/*",
      "main.js"
    ],
    "extraFiles": [
      {
        "from": "node_modules/electron/dist/ffmpeg.dll",
        "to": "."
      }
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "author": "andronisdre",
  "license": "ISC",
  "devDependencies": {
    "electron": "^33.0.2",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "firebase": "^11.0.1"
  }
}
